{
  "name": "nnergix_crawler",
  "version": "1.0.0",
  "description": "Technical test for Nnergix.",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "clean": "rimraf dist",
    "build": "tsc -v && npm run clean && npm run build-ts",
    "build-ts": "tsc",
    "watch": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:fix": "tslint --fix src/**/*.{ts,js}",
    "test": "NODE_ENV=test mocha -r ts-node/register src/tests/*.spec.ts --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josel567/nnergix_crawler.git"
  },
  "author": "Jose Luis Garcia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/josel567/nnergix_crawler/issues"
  },
  "homepage": "https://github.com/josel567/nnergix_crawler#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-router": "^0.1.15",
    "mongoose": "^5.9.17",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.18",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.21",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  }
}
