{
  "name": "nnergix_crawler",
  "version": "1.0.0",
  "description": "Technical test for Nnergix.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -v && npm run clean && npm run build-ts",
    "build-ts": "tsc",
    "watch": "NODE_ENV=development nodemon --exec ts-node src/server/index.ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:fix": "tslint --fix src/**/*.{ts,js}",
    "test": "NODE_ENV=test mocha -r ts-node/register src/tests/*.spec.ts --exit",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "ts-node src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josel567/nnergix_crawler.git"
  },
  "author": "Jose Luis Garcia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/josel567/nnergix_crawler/issues"
  },
  "homepage": "https://github.com/josel567/nnergix_crawler#readme",
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-router": "^0.1.15",
    "mongoose": "^5.9.17",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reflect-metadata": "^0.1.13",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@types/cheerio": "^0.22.18",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/mongoose": "^5.7.21",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/supertest": "^2.0.9",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [],
    "testRegex": "/test/specs/(.*/)?\\w+\\.tsx?",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "{bin,src}/**/*.{ts,tsx}"
    ]
  }
}
